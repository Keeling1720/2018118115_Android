<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.constellation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/TranslucentTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".home.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".luckfrag.LuckAnalysisActivity" />
        <activity android:name=".partnerfrag.PartnerAnalysisActivity" />
        <activity android:name=".starfrag.StarAnalysisActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>